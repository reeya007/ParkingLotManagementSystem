<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox prefHeight="600.0" prefWidth="800.0" spacing="10" xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.parkinglot.controllers.VehicleInfoController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="Vehicle Information" styleClass="section-subheader"/>
    <TableView fx:id="vehiclesTable" prefHeight="400.0">
        <columns>
            <TableColumn fx:id="licensePlateColumn" text="License Plate"/>
            <TableColumn fx:id="vehicleTypeIdColumn" text="Vehicle Type"/>
        </columns>
    </TableView>
    <Button text="Back" onAction="#handleBackButton"/>
</VBox>