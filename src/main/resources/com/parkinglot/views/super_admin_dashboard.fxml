<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.parkinglot.controllers.SuperAdminDashboardController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <VBox alignment="CENTER" spacing="10.0">
        <Button text="Add New Admin" onAction="#handleAddAdmin" />
        <Button text="View All Users" onAction="#handleViewUsers" />
        <Button text="Add Vehicle Type" onAction="#handleAddVehicleType" />
        <Button text="Add Parking Slot" onAction="#handleAddParkingSlot" />
    </VBox>
    <Button text="Back" onAction="#handleBackButton" />
</VBox>